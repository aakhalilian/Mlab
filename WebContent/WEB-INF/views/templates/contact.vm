		  <div class="form-group">
		    <div class="col-sm-12">
		      <input type="email" class="form-control" name="email" id="inputEmail" placeholder="Email" required>
		    </div>
		  </div>
		  <div class="form-group">
		    <div class="col-sm-12">
		      <input type="text" class="form-control" name="subject" id="inputTitle" placeholder="Subject">
		    </div>
		  </div>
		  <div class="form-group">
		    <div class="col-sm-12">
		      <textarea class="form-control" rows="5" style="resize:none" name="body" id="inputBody" placeholder="Message Body.."></textarea>
		    </div>
		  </div>
		  <div class="form-group">
		    <div class="col-sm-12">
		      <button type="submit" class="btn btn-primary">Send</button>
